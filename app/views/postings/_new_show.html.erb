
<%= form_for @posting.uploads.build, :url => uploads_path, html: { multipart: true } do |f| %>
  <%= f.hidden_field :posting_id %>
  <%= f.label :uploaded_file, t('.upload_new_file') %>
  <%= f.file_field :uploaded_file, multiple: true, name: 'upload[uploaded_file]' %>
  <%= f.submit 'Save', class: 'btn' %>
<% end %>

<% # jquery upload template # %>
<script id="template-upload" type="text/x-tmpl"><div class="upload">{%=o.name%}<div class="progress"><div class="bar" style="width: 0%"></div></div></div></script>
<div id="page" onclick="void(0)">


<div id="content">
  <div id="uploadimage">
    <div class="image-navigation">
      <a href="#" class="previous"></a>
      <a href="#" class="next"></a>
    </div>
    <div class="vehicle-make-model-and-name">
      <h1 class="year-make-model">
      </h1>
      <h2 class="vehicle-name">
      </h2>
    </div>
  </div>
  <div id="left-column">
    <div id="vehicle-details">
    </div>
  </div>
  <div id="right-column">
  </div>
</div>
